{
    "family": "aymen-task-def-pipeline",
    "containerDefinitions": [
        {
           "name": "mynote-app",
            "image": "aymenboujmil/mynote",
            "portMappings": [
                {
                    "containerPort": 5000,
                    "hostPort": 5000,
                    "protocol": "tcp"
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "memory": "1024",
    "cpu": "512"
}